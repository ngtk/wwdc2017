{
	"name": "WWDC2017",
	"type": "dark",
	"colors": {
		"editor.background": "#23252B",
		"editor.foreground": "#FFFFFF",
		"activityBarBadge.background": "#515B70",
		"sideBarTitle.foreground": "#FFFFFF"
	},
	"tokenColors": [
		{
			"name": "Other",
			"scope": [
				"punctuation.section.scope",
				"punctuation.definition.arguments",
				"punctuation.separator.argument-label",
				"meta.definition.type.body",
				"punctuation.separator.generic-parameter-clause",
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#6C7986"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#FC6A5D"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#D0BF69"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.modifier",
				"entity.name.function",
				"storage.type",
			],
			"settings": {
				"foreground": "#FC5FA3"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Type Definition",
			"scope": [
				"meta.definition.type.struct",
				"entity.name.type",
				"entity.name.class",
			],
			"settings": {
				"foreground": "#5DD8FF"
			}
		},
		{
			"name": "Type Definition",
			"scope": [
				"meta.definition.type.body",
				"variable.other",
			],
			"settings": {
				"foreground": "#41A1C0"
			}
		},
		{
			"name": "Project Function and Method Names",
			"scope": [
				"meta.function-call",
			],
			"settings": {
				"foreground": "#67B7A4"
			}
		},
		{
			"name": "Project Class Names",
			"scope": [
				"meta.inheritance-clause",
				"constant.other",
			],
			"settings": {
				"foreground": "#9EF1DD"
			}
		},
	]
}